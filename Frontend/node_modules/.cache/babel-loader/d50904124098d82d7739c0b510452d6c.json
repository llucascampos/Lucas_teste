=\"passageiro.cpf\" label=\"CPF\" :rules=\"documentoValido\" v-mask=\"'###.###.###-##'\"></v-text-field>\r\n                </v-flex>\r\n              </v-layout>\r\n              <v-layout wrap justify-center>\r\n                <v-flex xs12 sm6 md10>\r\n                  <v-text-field v-model=\"passageiro.nascimento\" label=\"Data de Nascimento\" :rules=\"dataValida\" v-mask=\"'##/##/####'\" ></v-text-field>\r\n                </v-flex>\r\n              </v-layout>\r\n              \r\n              <v-layout wrap justify-center>\r\n                <v-flex xs12 sm6 md5>\r\n                  <v-radio-group v-model=\"passageiro.sexo\" :mandatory=\"false\" row :rules=\"obrigatorio\">\r\n                    <v-radio label=\"Masculino\" value=\"Masculino\"></v-radio>\r\n                    <v-radio label=\"Feminino\"  value=\"Feminino\"></v-radio>\r\n                  </v-radio-group>\r\n                </v-flex>\r\n  \r\n              </v-layout>\r\n            </v-container>\r\n          </v-card-text>\r\n\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"blue darken-1\" flat @click=\"fechar\">Cancelar</v-btn>\r\n            <v-btn color=\"blue darken-1\" flat @click=\"salvar\">Salvar</v-btn>\r\n          </v-card-actions>\r\n          </v-form>\r\n        </v-card>\r\n      </v-dialog>\r\n\r\n    <v-data-table :headers=\"headers\" :items=\"listapassageiros\"  hide-actions class=\"elevation-1\" no-data-text=\"Sem Dados\">\r\n      <template v-slot:items=\"props\">\r\n        <td class=\"text-xs-left\">{{ props.item.nome }}</td>\r\n        <td class=\"text-xs-left\">{{ props.item.cpf }}</td>\r\n        <td class=\"text-xs-left\">{{ formatarData(props.item.dataNascimento) }}</td>\r\n        <td class=\"text-xs-left\">{{ props.item.sexo }}</td>\r\n        <td class=\"justify-center layout px-0\">\r\n          <v-icon  class=\"mr-2\" @click=\"editarItem(props.item)\">edit</v-icon>\r\n          <v-icon  @click=\"excluirItem(props.item)\">delete</v-icon>\r\n        </td>\r\n      </template>\r\n    </v-data-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n   data: () => ({\r\n      dialog: false,\r\n      headers: [\r\n        { text: 'Nome',                align: 'left', value: 'nome',       sortable: false},\r\n        { text: 'CPF',                 align: 'left', value: 'cpf',        sortable: false },\r\n        { text: 'Data de Nascimento',  align: 'left', value: 'nascimento', sortable: false },\r\n        { text: 'Sexo',                align: 'left', value: 'sexo',       sortable: false },\r\n        { text: 'Ações',                              value: 'acoes',      sortable: false }\r\n      ],\r\n\r\n      listapassageiros: [],\r\n      editarIndex: -1,\r\n      passageiro: {\r\n        nome: '',\r\n        cpf: '',\r\n        nascimento: '',\r\n        sexo: null,\r\n      },\r\n      defaultItem: {\r\n        nome: '',\r\n        cpf: null,\r\n        nascimento: null,\r\n        sexo: null,\r\n      },\r\n\r\n      //validação\r\n      obrigatorio: [\r\n        v => !!v || 'Campo Obrigatorio'\r\n      ],\r\n      \r\n      documentoValido:[\r\n        v => !!v || 'Campo Obrigatorio',\r\n        v => v.length == 14 || 'Insira um CPF Valido'\r\n      ],\r\n      dataValida:[\r\n        v => !!v || 'Campo Obrigatorio',\r\n        v => v.length == 10 || 'Insira a data Corretamente'\r\n      ],\r\n    \r\n       //snakebar\r\n      snackbar: false,\r\n      color: '',\r\n      mode: '',\r\n      timeout: 3000,\r\n      text: 'Salvo com Sucesso'\r\n    }),\r\n\r\n    computed: {\r\n      formTitle () {\r\n        return this.editarIndex === -1 ? 'Adicionar passageiro' : 'Editar passageiro'\r\n      }\r\n    },\r\n\r\n    watch: {\r\n      dialog (val) {\r\n        val || this.fechar()\r\n      }\r\n    },\r\n\r\n    created () {\r\n      this.inicializando()\r\n    },\r\n\r\n    methods: {\r\n      inicializando () {\r\n       let url = 'http://localhost:8081/passageiros'\r\n        axios\r\n          axios.get(url, \r\n          { \r\n            headers: {\r\n              'content-type': 'application/json',\r\n              'Accept': 'application/json',\r\n              'Access-Control-Allow-Credentials':true,\r\n              'Access-Control-Allow-Origin':true\r\n           }})\r\n          .then(response => {\r\n            this.listapassageiros = response.data.response\r\n            console.log(response.data)\r\n          })\r\n      .catch(error => {\r\n        console.log(error)\r\n        this.errored = true\r\n      })\r\n      },\r\n\r\n      editarItem (item) {\r\n        this.editarIndex = this.listapassageiros.indexOf(item)\r\n        this.passageiro = Object.assign({}, item)\r\n        this.dialog = true\r\n      },\r\n\r\n      excluirItem (item) {\r\n        let index = this.listapassageiros.indexOf(item)\r\n        confirm('Tem certeza que deseja excluir esse passageiro?') && this.listapassageiros.splice(index, 1)\r\n      },\r\n\r\n      fechar () {\r\n        this.dialog = false\r\n        setTimeout(() => {\r\n          this.$refs.form.resetValidation()\r\n          this.passageiro = Object.assign({}, this.defaultItem)\r\n          this.editarIndex = -1\r\n        }, 300)\r\n      },\r\n\r\n      salvar () {\r\n        if(this.$refs.form.validate()){\r\n          if (this.editarIndex > -1) {\r\n            Object.assign(this.listapassageiros[this.editarIndex], this.passageiro)\r\n          } else {\r\n\r\n            this.passageiro.nascimento = this.formataDataParaBanco(this.passageiro.nascimento)\r\n            this.passageiro.cpf = this.formatarCpf(this.passageiro.cpf)\r\n            axios.post('http://localhost:8081/passageiros', this.passageiro)\r\n              .then(function(response){\r\n                  this.snackbar = true\r\n                  this.inicializando()\r\n            });\r\n            \r\n          }\r\n          this.fechar()\r\n        }\r\n      },\r\n      formatarCpf(cpf){\r\n        return cpf.replace(/\\D/g, '')\r\n      },\r\n\r\n       formataDataParaBanco(data){\r\n        var dia  = data.split(\"/\")[0];\r\n        var mes  = data.split(\"/\")[1];\r\n        var ano  = data.split(\"/\")[2];\r\n        return ano + '-' + (\"0\"+mes).slice(-2) + '-' + (\"0\"+dia).slice(-2)\r\n      },\r\n\r\n      formatarData(data){\r\n        let configData = data.split(\"-\");\r\n        let dataFormatada =\r\n          configData[2].split(\"T\")[0] +\r\n          \"/\" +\r\n          configData[1] +\r\n          \"/\" +\r\n          configData[0];\r\n        return dataFormatada;\r\n      },\r\n\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n"],"sourceRoot":"src/components"}]}